<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Plugins_are_not_supported</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
    SObjectHandler__r.SObjectType__r.DeveloperName != &apos;Log&apos;,
    SObjectHandler__r.SObjectType__r.DeveloperName != &apos;LogEntry&apos;,
    SObjectHandler__r.SObjectType__r.DeveloperName != &apos;LogEntryEvent&apos;,
    SObjectHandler__r.SObjectType__r.DeveloperName != &apos;LogEntryTag&apos;,
    SObjectHandler__r.SObjectType__r.DeveloperName != &apos;LoggerTag&apos;
)</errorConditionFormula>
    <errorDisplayField>SObjectHandler__c</errorDisplayField>
    <errorMessage>Plugins are not supported for this SObject Handler</errorMessage>
</ValidationRule>
